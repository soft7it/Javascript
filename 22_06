<h1><a href="url">TITLU</a></h1>


<script>
    function a(text, url){
        return "<a href="+url+">" + text + "</a>"
    }

    function h1(text){
        return "<h1>" + text + "</h1>";
    }

document.write (a("Google","http://google.com"));
document.write (h1("This is a title") );
document.write (h1(a("Google","http://google.com")) );

</script>

